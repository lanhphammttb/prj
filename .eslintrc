{
    "env": {
        "browser": true,
        "es2021": true
      },
    "plugins": ["@typescript-eslint"],
    "extends": ["react-app", "plugin:prettier/recommended", "eslint:recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":"warn",
        "@typescript-eslint/no-unused-vars":"warn"
    }
}